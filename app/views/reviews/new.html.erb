<h1>New Review</h1>
<%= simple_form_for [ @list, @review ] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>
<div class="button-container">
  <button type="button" class="btn btn-danger"><%= link_to "Back to My List", list_path(@list) %></button>
</div>
